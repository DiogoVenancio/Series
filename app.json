{
  "expo": {
    "name": "series",
    "slug": "series",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/launcher_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#6ca2f7"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dropg.series"
    },
    "android": {
      "package": "com.dropg.series"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
